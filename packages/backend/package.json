{
  "name": "backend",
  "version": "1.0.0",
  "description": "NestJS backend API",
  "scripts": {
    "build": "npx nest build",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:setup": "ts-node test/setup-test-db.ts",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/database/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register src/database/seeds/create-admin.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/multer": "^2.0.0",
    "aws-sdk": "^2.1400.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.9.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express-rate-limit": "^6.10.0",
    "firebase-admin": "^11.10.0",
    "helmet": "^7.0.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "@nestjs/cli": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^3.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
